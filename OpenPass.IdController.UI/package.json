{
  "name": "open-pass",
  "version": "0.0.0",
  "scripts": {
    "start": "run-p -r start:open-pass start:widget",
    "start:backendless": "run-p -r start:open-pass:backendless start:widget:backendless",
    "start:widget": "node ./dev/widget-launcher.js && ng serve --project widget --proxy-config proxy/proxy.conf.js",
    "start:widget:backendless": "node ./dev/widget-launcher.js && ng serve --project widget --proxy-config proxy/proxy.backendless.conf.js",
    "start:open-pass": "ng serve --project open-pass --proxy-config proxy/proxy.conf.js",
    "start:open-pass:backendless": "ng serve --project open-pass --proxy-config proxy/proxy.backendless.conf.js",
    "build": "npm run build:open-pass && npm run build:widget && npm run build:widget-launcher",
    "build:open-pass": "ng build --prod",
    "build:widget": "ng build --prod --project widget --single-bundle",
    "build:widget-launcher": "node ./dev/widget-launcher.js --build",
    "test": "ng test --watch=false",
    "lint": "npm run lint:typescript && npm run lint:prettier",
    "lint:typescript": "ng lint",
    "lint:prettier": "prettier --list-different \"./{projects,cypress}/**/*.{ts,component.html}\"",
    "prettier": "prettier --write \"./{projects,cypress}/**/*.{ts,component.html}\" --loglevel silent",
    "cypress:open": "cypress open",
    "cypress:wait-and-run": "wait-on https://localhost:4200/open-pass && cypress run --browser chrome",
    "cypress:run-tests": "run-p -r start:open-pass cypress:wait-and-run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,scss,ts,html,json}": "npm run prettier"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~11.1.0",
    "@angular/compiler": "~11.1.0",
    "@angular/core": "~11.1.0",
    "@angular/elements": "^11.1.0",
    "@angular/forms": "~11.1.0",
    "@angular/platform-browser": "~11.1.0",
    "@angular/platform-browser-dynamic": "~11.1.0",
    "@ngneat/until-destroy": "^8.1.1",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@ngxs-labs/dispatch-decorator": "^2.1.0",
    "@ngxs/store": "^3.7.1",
    "@webcomponents/custom-elements": "^1.4.3",
    "@webcomponents/shadydom": "1.1.0",
    "core-js": "^3.8.3",
    "rxjs": "~6.6.3",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.1",
    "@angular-eslint/builder": "^1.1.0",
    "@angular-eslint/eslint-plugin": "1.1.0",
    "@angular-eslint/eslint-plugin-template": "1.1.0",
    "@angular-eslint/schematics": "^1.1.0",
    "@angular-eslint/template-parser": "^1.1.0",
    "@angular/cli": "~11.1.1",
    "@angular/compiler-cli": "~11.1.0",
    "@angular/router": "^11.1.0",
    "@cypress/webpack-preprocessor": "^5.5.0",
    "@rollup/plugin-replace": "^2.4.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@types/gapi": "0.0.39",
    "@types/gapi.auth2": "0.0.54",
    "@types/jasmine": "~3.6.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "codelyzer": "^6.0.1",
    "cypress": "^6.5.0",
    "eslint": "^7.18.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^31.3.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "4.3.8",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.5.4",
    "ngx-build-plus": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "puppeteer": "^8.0.0",
    "rollup": "^2.39.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.1.3",
    "wait-on": "^5.2.1"
  }
}
