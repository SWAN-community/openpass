{
  "name": "id-controller-ui",
  "version": "0.0.0",
  "scripts": {
    "start": "npm run start:id-controller-ui | npm run start:widget",
    "start:preprod": "npm run start:id-controller-ui:preprod | npm run start:widget:preprod",
    "start:backendless": "npm run start:id-controller-ui:backendless | npm run start:widget:backendless",
    "start:widget": "ng serve --project widget --proxy-config proxy/proxy.conf.js",
    "start:widget:preprod": "ng serve --project widget --proxy-config proxy/proxy.preprod.conf.js",
    "start:widget:backendless": "ng serve --project widget --proxy-config proxy/proxy.backendless.conf.js",
    "start:id-controller-ui": "ng serve --project id-controller-ui --proxy-config proxy/proxy.conf.js",
    "start:id-controller-ui:preprod": "ng serve --project id-controller-ui --proxy-config proxy/proxy.preprod.conf.js",
    "start:id-controller-ui:backendless": "ng serve --project id-controller-ui --proxy-config proxy/proxy.backendless.conf.js",
    "build": "npm run build:id-controller-ui && npm run build:widget && npm run build:widget-launcher",
    "build:id-controller-ui": "ng build --prod",
    "build:widget": "ng build --prod --project widget --single-bundle && ng build --project widget --single-bundle -c preprod",
    "build:widget-launcher": "node ./dev/widget-launcher.js --build",
    "test": "ng test --watch=false",
    "lint": "npm run lint:typescript && npm run lint:prettier",
    "lint:typescript": "ng lint",
    "lint:prettier": "prettier --list-different \"./{projects,cypress}/**/*.{ts,component.html}\"",
    "prettier": "prettier --write \"./{projects,cypress}/**/*.{ts,component.html}\" --loglevel silent"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,scss,ts,html,json}": "npm run prettier"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~11.1.0",
    "@angular/compiler": "~11.1.0",
    "@angular/core": "~11.1.0",
    "@angular/elements": "^11.1.0",
    "@angular/forms": "~11.1.0",
    "@angular/platform-browser": "~11.1.0",
    "@angular/platform-browser-dynamic": "~11.1.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@ngxs-labs/dispatch-decorator": "^2.1.0",
    "@ngxs/store": "^3.7.1",
    "@webcomponents/custom-elements": "^1.4.3",
    "@webcomponents/shadydom": "1.1.0",
    "core-js": "^3.8.3",
    "rxjs": "~6.6.3",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.1",
    "@angular-eslint/builder": "^1.1.0",
    "@angular-eslint/eslint-plugin": "1.1.0",
    "@angular-eslint/eslint-plugin-template": "1.1.0",
    "@angular-eslint/schematics": "^1.1.0",
    "@angular-eslint/template-parser": "^1.1.0",
    "@angular/cli": "~11.1.1",
    "@angular/compiler-cli": "~11.1.0",
    "@angular/router": "^11.1.0",
    "@cypress/webpack-preprocessor": "^5.5.0",
    "@rollup/plugin-replace": "^2.4.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@types/jasmine": "~3.6.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "codelyzer": "^6.0.1",
    "cypress": "^6.5.0",
    "eslint": "^7.18.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^31.3.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "^5.0.8",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.5.4",
    "ngx-build-plus": "^11.0.0",
    "prettier": "^2.2.1",
    "rollup": "^2.39.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.1.3"
  }
}
