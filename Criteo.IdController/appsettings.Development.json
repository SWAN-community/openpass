{
  "CriteoLogging": {
    "Section": [
      {
        "Target": [
          {
            "Type": "console",
            "Minimum": "Debug"
          },
          {
            "Type": "file",
            "Minimum": "Debug",
            "Param": [
              {
                "Name": "path",
                "Value": "{default(env('CRITEOROOT'), '.')}\\log\\Criteo.IdController\\{level.name}\\{format(timestamp, 'dl:yyyy-MM-dd-HH', '')}.log"
              }
            ]
          }
        ]
      }
    ]
  },
  "Email": {
    "Server": {
      "Host": "mail-relay.service.consul",
      "Port": 25,
      "EnableSsl": false
    },
    "Sender": {
      "DisplayName": "OpenPass",
      "Address": "id-persistence@criteo.com"
    }
  },
  "adminUrl": "http://localhost:8080",
  "urls": "http://localhost:1234;"
}
